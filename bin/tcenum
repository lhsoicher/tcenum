#!/bin/sh
if test -z $1
then
echo "usages:"
echo "tcenum <presentation_file>"
echo "tcenum <presentation_file> <maxc>"
echo "tcenum <presentation_file> <maxc> <ilooka>"
echo "tcenum <presentation_file> <maxc> <ilooka> <result_file>"
echo "tcenum <presentation_file> <maxc> <ilooka> <result_file> <permgens_file>"
exit
fi
tcfrontend_in=$(mktemp -p/tmp tcfrontend_in.XXXXXXXXXX)
cp $1 $tcfrontend_in
tcfrontend_out=$(mktemp -p/tmp tcfrontend_out.XXXXXXXXXX)
tcfrontend $tcfrontend_in $tcfrontend_out
if [ $? -ne 0 ] 
then 
echo "tcfrontend failed" >&2
exit 1
fi
if test $2 
then 
maxc=$2
else 
maxc=0
fi
if test $3
then 
ilooka=$3
else 
ilooka=0
fi
if test $5
then 
outsw=1
else
outsw=0
fi
tcmain_in=$(mktemp -p/tmp tcmain_in.XXXXXXXXXX)
echo "$ilooka $outsw 2 $maxc" > $tcmain_in
cat $tcfrontend_out >> $tcmain_in
tcmain_out=$(mktemp -p/tmp tcmain_out.XXXXXXXXXX)
if [ $outsw -eq 0 ]
then 
tcmain_permgens=""
else 
tcmain_permgens=$(mktemp -p/tmp tcmain_permgens.XXXXXXXXXX)
fi
tcmain $tcmain_in $tcmain_out $tcmain_permgens
if [ $? -ne 0 ] 
then 
echo "tcmain failed" >&2
exit 1
fi
cat $tcmain_out
if test $4
then 
cat $tcfrontend_in $tcmain_out >>$4 
echo " " >>$4
fi
if [ $outsw -eq 1 ]
then 
mv $tcmain_permgens $5
fi
rm $tcfrontend_in $tcfrontend_out $tcmain_in $tcmain_out
